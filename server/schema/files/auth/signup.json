{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "Signup",
	"description": "Signup.",
	"additionalProperties": false,
	"required": ["email", "password", "name", "phone"],
	"properties": {
		"name": {
			"type": "string"
		},
		"password": {
			"type": "string",
			"minLength": 6
		},
		"email": {
			"type": "string",
			"format": "email"
		},
		"phone": {
			"type": "string",
			"maxLength": 10,
			"minLength": 10
		}
	}
}
